[run]
branch = True

[report]
# Wyłącz CompanyAccount z code coverage (Feature 18)
# Wyłącz smtp/ z coverage (Feature 19 - stub)
omit =
    */company_account.py
    */smtp/*
    */site-packages/*

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
